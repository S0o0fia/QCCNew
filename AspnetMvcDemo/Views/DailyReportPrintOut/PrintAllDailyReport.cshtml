@model  List<AspnetMvcDemo.Models.Printout>
<style>
    table {
        border-collapse: collapse;
    }

    table, th, td {
        border: 2px solid black;
        text-align: center;
        vertical-align: middle;
    }

    h2 {
        text-decoration: underline;
    }

    .true {
        background-color: #30d10c;
    }

    .false {
        background-color: #f70202;
    }

    td, span {
        font-size: x-large;
    }
</style>
<body dir="rtl">

    <!--Page1-->
    <div class="row" style="height:100%;">
        <div class=" row" style="display:inline-block; width:100% ; margin-bottom:20px;">
            <div class="col-3" style="display:inline-block; float:right; padding-right:2%">
                @{
                    var amana = Server.MapPath("~/Images/client-1.jpg");
                }
                <img style="width:100px; height:100px" src="@amana" />

            </div>
            <div class="col-6" style="display:inline-block; text-align:center; padding-right:20%">
                <h1>أمانة منطقة الرياض</h1>
                <h2>
                    الإدارة العامة للتشغيل والصيانة - إدارة البحوث والمواد
                </h2>
                <h4>
                    مشروع ضبط وتأكيد جودة إنتاج مصانع الخرسانة الجاهزة والبلوك بمدينة الرياض
                </h4>
            </div>
            <div class="col-3" style="display:inline-block; float:left">
                @{
                    var saudi = Server.MapPath("~/Images/client-3.jpg");
                }
                <img style="width:140px; height:120px" src="@saudi" />
            </div>

            <hr style="border:1px solid #808080" />
        </div>
        <div class="row">
            <div class="col-12" style="text-align:center">
                <h2> الاستشاري : التقنيون السعوديون (سعودي تك) لفحص التربية والمواد </h2>
            </div>
        </div>
        <br />
        <div class="row">
            <div class="col-3" style="text-align:right;"><h5>@Model[0].ReportDate.Value.ToString("yyyy/MM/dd")</h5> </div>

            <div class="col-9" style="text-align:center"><h2>ملخص تقارير الزيارات اليومية لمصانع الخرسانة</h2></div>
        </div>

        <div class="row">
            <div>
                <table class="table table-bordered">
                    <thead style="background-color:#bdc9cb;">
                        <tr>
                            <th rowspan="2">م</th>
                            <th rowspan="2">اسم المصنع</th>
                            <th rowspan="2">رقم العينة</th>
                            <th rowspan="2">(MPA)رتبة الخرسانة</th>
                            <th rowspan="2">نوع الاسمنت</th>
                            <th rowspan="2">عنصر الاستخدام</th>

                            <th colspan="2">الاستخدام</th>
                            <th rowspan="2">درجة الحراره</th>
                            <th rowspan="2">الهبوط</th>
                            <th rowspan="2">نظافة الموقع</th>
                            <th rowspan="2">التحكم في الغبار</th>
                            <th rowspan="2">رش الركام</th>
                            <th rowspan="2">تواجد مهندس المختبر</th>
                            <th rowspan="2">وجود ملدنات</th>
                            <th rowspan="2">لبس الكمامات</th>
                        </tr>
                        <tr>
                            <th>مطابق</th>
                            <th>غير مطابق</th>
                        </tr>
                    </thead>
                    <tbody>
                        @{var count = 0;
                            foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @{
                                            count = count + 1;
                                        }
                                        @count
                                    </td>
                                    <td>
                                        @item.FactoryName
                                    </td>
                                    <td>
                                        @item.SampleNumber
                                    </td>
                                    <td>
                                        @item.ConcreteRank
                                    </td>
                                    <td>
                                        @item.CementType
                                    </td>
                                    @if (item.IsCleanUsage == true)
                                    {
                                        <td>نظافة</td>
                                    }
                                    else if (item.IsColumnUsage == true)
                                    {
                                        <td>أعمدة</td>
                                    }
                                    else if (item.IsRoofUsage == true)
                                    {
                                        <td>أسقف</td>
                                    }
                                    else if (item.IsBasicUsage == true)
                                    {
                                        <td>أساسات</td>
                                    }
                                    else
                                    {
                                        <td> @item.OtherReason</td>
                                    }

                                    @if ((item.ConcreteRank == "C-15" && item.IsCleanUsage == true) ||
                                        (item.ConcreteRank == "C-25" && item.IsRoofUsage == true) ||
                                        (item.ConcreteRank == "C-30" && item.IsBasicUsage == true) ||
                                        (item.ConcreteRank == "C-15" && item.IsColumnUsage == true) ||
                                        (item.ConcreteRank == "C-15" && item.IsOtherUsage == true) ||
                                        (item.ConcreteRank == "C-35"))
                                    {
                                        <td class="true">
                                            <i class="ion-ios-checkmark-circle" style="color:white; font-size:large">&#10004;</i>
                                        </td>
                                        <td></td>
                                    }
                                    else
                                    {
                                        <td></td>
                                        <td class="false">
                                            <i class="ion-ios-close-circle" style="color:white; font-size:large">&#10006;</i>
                                        </td>
                                    }

                                    @if (item.ConcreteTemperture <= 34)
                                    {
                                        <td style="background-color:forestgreen">
                                            @item.ConcreteTemperture
                                        </td>
                                    }
                                    else
                                    {
                                        <td style="background-color:red">
                                            @item.ConcreteTemperture
                                        </td>
                                    }
                                    @if (item.DownAmount >= 100)
                                    {
                                        <td style="background-color:forestgreen">
                                            @item.DownAmount
                                        </td>
                                    }
                                    else
                                    {
                                        <td style="background-color:red">
                                            @item.DownAmount
                                        </td>
                                    }

                                    <!--for Clean location-->
                                    @if (item.IsCleanLocation == true)
                                    {
                                        <td class="true">
                                            <i class="ion-ios-checkmark-circle" style="color:white; font-size:large">&#10004;</i>
                                        </td>
                                    }
                                    else
                                    {
                                        <td class="false">
                                            <i class="ion-ios-close-circle" style="color:white; font-size:large">&#10006;</i>
                                        </td>
                                    }

                                    <!--IsDustControlInStation-->
                                    @if (item.IsDustControlInStation == true)
                                    {
                                        <td class="true">
                                            <i class="ion-ios-checkmark-circle" style="color:white; font-size:large">&#10004;</i>
                                        </td>
                                    }
                                    else
                                    {
                                        <td class="false">
                                            <i class="ion-ios-close-circle" style="color:white; font-size:large">&#10006;</i>
                                        </td>
                                    }

                                    <!--IsRokamSummer-->
                                    @if (item.IsRokamSummer == true)
                                    {
                                        <td class="true">
                                            <i class="ion-ios-checkmark-circle" style="color:white; font-size:large">&#10004;</i>
                                        </td>
                                    }
                                    else
                                    {
                                        <td class="false">
                                            <i class="ion-ios-close-circle" style="color:white; font-size:large">&#10006;</i>
                                        </td>
                                    }

                                    <!--IsLabEngineer-->
                                    @if (item.IsLabEngineer == true)
                                    {
                                        <td class="true">
                                            <i class="ion-ios-checkmark-circle" style="color:white; font-size:large">&#10004;</i>
                                        </td>
                                    }
                                    else
                                    {
                                        <td class="false">
                                            <i class="ion-ios-close-circle" style="color:white; font-size:large">&#10006;</i>
                                        </td>
                                    }


                                    <!--IsMoldanatInTrucks-->
                                    @if (item.IsMoldanatInTrucks == true)
                                    {
                                        <td class="true">
                                            <i class="ion-ios-checkmark-circle" style="color:white; font-size:large">&#10004;</i>
                                        </td>
                                    }
                                    else
                                    {
                                        <td class="false">
                                            <i class="ion-ios-close-circle" style="color:white; font-size:large">&#10006;</i>
                                        </td>
                                    }

                                    <!--IsPeopleSafty-->
                                    @if (item.IsPeopleSafty == true)
                                    {
                                        <td class="true">
                                            <i class="ion-ios-checkmark-circle" style="color:white; font-size:large">&#10004;</i>
                                        </td>
                                    }
                                    else
                                    {
                                        <td class="false">
                                            <i class="ion-ios-close-circle" style="color:white; font-size:large">&#10006;</i>
                                        </td>
                                    }
                                </tr>

                            }

                        }
                    </tbody>
                </table>
            </div>
        </div>
        <br />
        <br />

        <div class="row">
            <div class="col-6" style="text-align:right; "><h3>مدير المشروع</h3></div>
            <div class="col-6" style="text-align:left;"><h3>مشرف المشروع</h3></div>
        </div>
        <div class="row">
            <div class="col-6" style="text-align:right; "><h3>م. وائل بن سليمان الحمدان</h3></div>
            <div class="col-6" style="text-align:left;"><h3> م. حمد بن محمد الحربي</h3></div>
        </div>
    </div>
    <br />


