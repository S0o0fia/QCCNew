    <script src="https://maps.google.com/maps/api/js?key=AIzaSyABsPRMD1jkSGb6-D88OoFngcCKTU2dT0U"></script>
    @Scripts.Render("~/bundle/vendor/js/gmaps/gmaps")
    @*@Scripts.Render("~/bundle/js/charts_gmaps")*@
